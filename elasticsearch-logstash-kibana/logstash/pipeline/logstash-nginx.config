input {
 
  tcp {
    port => 5044
    codec => json
  }
}

filter {
  json {
    source => "message"
  }
  geoip {
    source => "remote_ip"
  }
  useragent {
    source => "agent"
    target => "useragent"
  }
  date {
    match => [ "timestamp", "ISO8601" ]
  }
}

output {
  elasticsearch {
    hosts => ["http://es:9200"]
    index => "spring-boot-%{+YYYY.MM.dd}"
  }
  stdout {
    codec => rubydebug
  }
}